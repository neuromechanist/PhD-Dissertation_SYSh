@ARTICLE{Larsen2018-oe,
  title     = "Modulation of task-related cortical connectivity in the acute
               and subacute phase after stroke",
  author    = "Larsen, Lisbeth H and Zibrandtsen, Ivan C and Wienecke, Troels
