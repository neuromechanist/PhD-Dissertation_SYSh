@ARTICLE{Betschart2017-os,
  title     = "Changes in lower limb muscle activity after walking on a
               split-belt treadmill in individuals post-stroke",
  author    = "Betschart, Martina and Lauzi{\`e}re, S{\'e}l{\'e}na and
