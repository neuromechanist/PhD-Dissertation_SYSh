@ARTICLE{Valizadeh2019-sn,
  title    = "Decrypting the electrophysiological individuality of the human
              brain: Identification of individuals based on resting-state {EEG}
              activity",
  author   = "Valizadeh, Seyed Abolfazl and Riener, Robert and Elmer, Stefan
              and J{\"a}ncke, Lutz",
  abstract = "Biometric identification (BI) of individuals is a fast-growing
              field of research that is producing increasingly sophisticated
              applications in several spheres of everyday life. Previous
              magnetic resonance imaging (MRI) studies have demonstrated that
              based on the high inter-individual variability of brain structure
