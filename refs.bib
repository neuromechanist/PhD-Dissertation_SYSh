}

@ARTICLE{Rowbottom2012-di,
  title     = "The Role of Hypotheses in Biomechanical Research",
  author    = "Rowbottom, Darrell P and McNeill Alexander, R",
  abstract  = "This paper investigates whether there is a discrepancy between
               stated and actual aims in biomechanical research, particularly
               with respect to hypothesis testing. We present an analysis of
               one hundred papers recently published in The Journal of
               Experimental Biology and Journal of Biomechanics, and examine
               the prevalence of papers which (a) have hypothesis testing as a
