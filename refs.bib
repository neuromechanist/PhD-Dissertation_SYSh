@ARTICLE{Oliveira2017-jh,
  title    = "Restricted vision increases sensorimotor cortex involvement in
              human walking",
  author   = "Oliveira, Anderson S and Schlink, Bryan R and Hairston, W David
              and K{\"o}nig, Peter and Ferris, Daniel P",
  abstract = "This study aimed to determine whether there is electrocortical
              evidence of augmented participation of sensory brain areas in
              walking modulation during walking with eyes closed. Healthy
              subjects (n = 10) walked on a treadmill at 1 m/s while
              alternating 5 min of walking with the eyes open or closed while
