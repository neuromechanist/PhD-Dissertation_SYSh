  volume   =  115,
  number   =  1,
  pages    = "379--388",
  month    =  jan,
  year     =  2016,
  keywords = "electroencephalography; fatigue; locomotion; motor control",
  language = "en"
}

@ARTICLE{Lin2017-fd,
