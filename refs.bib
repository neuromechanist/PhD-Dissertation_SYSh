@ARTICLE{Kolarova2013-za,
  title    = "Limits of stability in persons with transtibial amputation with
              respect to prosthetic alignment alterations",
  author   = "Kolarova, Barbora and Janura, Miroslav and Svoboda, Zdenek and
              Elfmark, Milan",
  abstract = "OBJECTIVES: To evaluate the limits of stability (LOS) in persons
