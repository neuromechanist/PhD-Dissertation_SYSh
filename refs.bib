@ARTICLE{Sharon2007-ns,
  title     = "The advantage of combining {MEG} and {EEG}: comparison to {fMRI}
               in focally stimulated visual cortex",
  author    = "Sharon, Dahlia and H{\"a}m{\"a}l{\"a}inen, Matti S and Tootell,
