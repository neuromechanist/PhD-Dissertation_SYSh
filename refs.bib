}

@ARTICLE{Cohen1990-yd,
  title    = "{MEG} versus {EEG} localization test using implanted sources in
              the human brain",
  author   = "Cohen, D and Cuffin, B N and Yunokuchi, K and Maniewski, R and
              Purcell, C and Cosgrove, G R and Ives, J and Kennedy, J G and
