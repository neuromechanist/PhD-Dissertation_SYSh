@ARTICLE{Curtze2010-hz,
  title    = "Balance recovery after an evoked forward fall in unilateral
              transtibial amputees",
  author   = "Curtze, Carolin and Hof, At L and Otten, Bert and Postema, Klaas",
  abstract = "Falls are a common and potentially dangerous event, especially in
