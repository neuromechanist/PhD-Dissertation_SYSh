@ARTICLE{Huang2009-jz,
  title    = "Upper limb effort does not increase maximal voluntary muscle
              activation in individuals with incomplete spinal cord injury",
  author   = "Huang, Helen J and Ferris, Daniel P",
  abstract = "OBJECTIVE: To determine the effect of upper limb effort on
