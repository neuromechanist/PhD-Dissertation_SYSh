@ARTICLE{Dietz1994-ys,
  title    = "Human neuronal interlimb coordination during split-belt
              locomotion",
  author   = "Dietz, V and Zijlstra, W and Duysens, J",
  abstract = "Human interlimb coordination and the adaptations in leg muscle
