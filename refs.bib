}

@INPROCEEDINGS{Pion-Tonachini2018-ss,
  title     = "Online Automatic Artifact Rejection using the Real-time {EEG}
               Source-mapping Toolbox ({REST})",
  booktitle = "2018 40th Annual International Conference of the {IEEE}
               Engineering in Medicine and Biology Society ({EMBC})",
  author    = "Pion-Tonachini, L and Hsu, S and Chang, C and Jung, T and
