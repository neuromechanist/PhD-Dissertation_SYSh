@UNPUBLISHED{Mathis2018-zr,
  title    = "On the inference speed and video-compression robustness of
              {DeepLabCut}",
  author   = "Mathis, Alexander and Warren, Richard",
  abstract = "Pose estimation is crucial for many applications in neuroscience,
              biomechanics, genetics and beyond. We recently presented a highly
