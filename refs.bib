@INPROCEEDINGS{Castermans2011-sq,
  title     = "An analysis of {EEG} signals during voluntary rhythmic foot
               movements",
  booktitle = "2011 5th International {IEEE/EMBS} Conference on Neural
               Engineering",
  author    = "Castermans, T and Duvinage, M and Hoellinger, T and Petieau, M
