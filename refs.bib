}

@ARTICLE{Tan2006-zm,
  title    = "Wrist impact velocities are smaller in forward falls than
              backward falls from standing",
  author   = "Tan, Juay-Seng and Eng, Janice J and Robinovitch, Stephen N and
              Warnick, Brady",
  abstract = "The wrist is a common fracture site for both young and older
