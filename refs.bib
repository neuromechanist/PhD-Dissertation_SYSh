@ARTICLE{Kautz2006-yg,
  title     = "Does unilateral pedaling activate a rhythmic locomotor pattern
               in the nonpedaling leg in post-stroke hemiparesis?",
  author    = "Kautz, S A and Patten, C and Neptune, R R",
  abstract  = "Recent investigation in persons with clinically complete spinal
               cord injury has revealed that locomotor activity in one limb can
