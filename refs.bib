}

@ARTICLE{Henry2006-jr,
  title    = "Decreased limits of stability in response to postural
              perturbations in subjects with low back pain",
  author   = "Henry, Sharon M and Hitt, Juvena R and Jones, Stephanie L and
              Bunn, Janice Y",
  abstract = "BACKGROUND: Low back pain is associated with abnormal movement
