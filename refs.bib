@ARTICLE{Debas2010-ce,
  title     = "Brain plasticity related to the consolidation of motor sequence
               learning and motor adaptation",
  author    = "Debas, Karen and Carrier, Julie and Orban, Pierre and Barakat,
               Marc and Lungu, Ovidiu and Vandewalle, Gilles and Hadj Tahar,
