@ARTICLE{Jain2013-pb,
  title     = "{EEG} during pedaling: evidence for cortical control of
               locomotor tasks",
  author    = "Jain, Sanket and Gourab, Krishnaj and Schindler-Ivens, Sheila
               and Schmit, Brian D",
  abstract  = "OBJECTIVE: This study characterized the brain electrical
               activity during pedaling, a locomotor-like task, in humans. We
