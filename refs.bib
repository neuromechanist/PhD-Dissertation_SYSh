@ARTICLE{Foged2017-pj,
  title    = "Safety and {EEG} data quality of concurrent high-density {EEG}
              and high-speed {fMRI} at 3 Tesla",
  author   = "Foged, Mette Thrane and Lindberg, Ulrich and Vakamudi, Kishore
