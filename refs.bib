  keywords = "Aging; Co-activation; Electromyography; Gait speed; Locomotion;
              Stability",
  language = "en"
}

@ARTICLE{Kitatani2016-vr,
  title    = "Ankle muscle coactivation during gait is decreased immediately
              after anterior weight shift practice in adults after stroke",
  author   = "Kitatani, Ryosuke and Ohata, Koji and Sakuma, Kaoru and Aga, Yumi
