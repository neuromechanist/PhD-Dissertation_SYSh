  language  = "en"
}

@ARTICLE{Reisman2010-pw,
  title     = "Split-belt treadmill training poststroke: a case study",
  author    = "Reisman, Darcy S and McLean, Heather and Bastian, Amy J",
  abstract  = "BACKGROUND AND PURPOSE: Even after rehabilitation, many
               individuals with strokes have residual gait deviations and
               limitations in functional walking. Applying the principles of
               motor adaptation through a split-belt treadmill walking paradigm
               can lead to short-term improvements in step length asymmetry
               after stroke. The focus of this case study was to determine
