  pages    = "S36--S40",
  month    =  feb,
  year     =  2015,
  keywords = "Tourette syndrome; Sensory organization test; Balance control"
}

@ARTICLE{Mayer2015-xm,
  title    = "Neurofeedback as a nonpharmacological treatment for adults with
