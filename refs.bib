  pages    = "147--161",
  month    =  jan,
  year     =  2013,
  language = "en"
}

@ARTICLE{Chaumon2015-fe,
  title    = "A practical guide to the selection of independent components of
              the electroencephalogram for artifact correction",
  author   = "Chaumon, Maximilien and Bishop, Dorothy V M and Busch, Niko A",
  abstract = "BACKGROUND: Electroencephalographic data are easily contaminated
              by signals of non-neural origin. Independent component analysis
