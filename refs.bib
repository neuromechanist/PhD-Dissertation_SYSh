  pages    = "3631",
  month    =  oct,
  year     =  2020
}

@INPROCEEDINGS{Takai2018-sq,
  title     = "Investigation on the Neural Correlates of Haptic Training",
  booktitle = "2018 {IEEE} International Conference on Systems, Man, and
               Cybernetics ({SMC})",
  author    = "Takai, A and Rivela, D and Lisi, G and Noda, T and Teramae, T
               and Imamizu, H and Morimoto, J",
  abstract  = "Haptic guidance is a motor training procedure in which the
               subject is physically guided through an ideal motion by an
               haptic interface. It is especially valuable for repetitive
               training, and more effective when coupled with additional
               sensory feedback, e.g. the visual modality. The advantage of
